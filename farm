db = false
autoFarmEnabled = false
infiniteJumpEnabled = false
local infiniteJumpConnection

ScreenGui.Name = "HaxGui"
ScreenGui.ResetOnSpawn = false
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
ScreenGui.Parent = game.Players.LocalPlayer.PlayerGui

if game.PlaceId > 537413528 or game.PlaceId < 537413528 then
    Button.Text = "Bro, Its Not Babft."
    Button.Active = false
end

function AutoFarm()
    while autoFarmEnabled do
        if game.PlaceId == 537413528 then
            Button.Text = 'Successfully Farming (Click To Stop)'
            Button.Draggable = false
            Button.Active = false
            Button.BackgroundColor3 =  Color3.fromRGB(85, 255, 127)
            wait(1)
            local endi = game.Workspace.BoatStages.NormalStages.TheEnd.GoldenChest.Trigger
            wait(1)
            repeat wait(0.1) until game.Workspace:FindFirstChild(Name):FindFirstChild("HumanoidRootPart")
            local body = game.Workspace:FindFirstChild(Name).HumanoidRootPart
            repeat wait(0.1) until game.Workspace:FindFirstChild(Name):FindFirstChild("Humanoid")
            local hum = game.Workspace:FindFirstChild(Name).Humanoid
            local gyro = Instance.new("BodyPosition")
            gyro.Parent = body
            body.CFrame = CFrame.new(0,0,0)
            wait(3)
            gyro.Position = game.Workspace.BoatStages.NormalStages.CaveStage1.DarknessPart.Position
            wait(3)
            gyro.Position = game.Workspace.BoatStages.NormalStages.CaveStage2.DarknessPart.Position
            wait(3)
            gyro.Position = game.Workspace.BoatStages.NormalStages.CaveStage3.DarknessPart.Position
            wait(3)
            gyro.Position = game.Workspace.BoatStages.NormalStages.CaveStage4.DarknessPart.Position
            wait(3)
            gyro.Position = game.Workspace.BoatStages.NormalStages.CaveStage5.DarknessPart.Position
            wait(3)
            gyro.Position = game.Workspace.BoatStages.NormalStages.CaveStage6.DarknessPart.Position
            wait(3)
            gyro.Position = game.Workspace.BoatStages.NormalStages.CaveStage7.DarknessPart.Position
            wait(3)
            gyro.Position = game.Workspace.BoatStages.NormalStages.CaveStage8.DarknessPart.Position
            wait(3)
            gyro.Position = game.Workspace.BoatStages.NormalStages.CaveStage9.DarknessPart.Position
            wait(3)
            gyro.Position = game.Workspace.BoatStages.NormalStages.CaveStage10.DarknessPart.Position
            wait(2.5)
            gyro.Position = endi.Position
            body.CFrame = endi.CFrame
        end
    end
end

function OnClicked(plr)
    if autoFarmEnabled == false then
        autoFarmEnabled = true
        Button.Text = "AutoFarm Enabled"
        Button.BackgroundColor3 = Color3.fromRGB(85, 255, 127)
        db = true
        spawn(AutoFarm)
    else
        autoFarmEnabled = false
        db = false
        Button.Text = "Start Farming"
        Button.BackgroundColor3 = Color3.fromRGB(100, 177, 0)
    end
end

Button.MouseButton1Click:Connect(OnClicked)

game.Players.LocalPlayer.CharacterAdded:Connect(function()
    if autoFarmEnabled then
        spawn(AutoFarm)
    end
end)

InfiniteJumpButton.MouseButton1Click:Connect(function()
    if infiniteJumpEnabled == false then
        infiniteJumpEnabled = true
        local player = game.Players.LocalPlayer
        local humanoid = player.Character:FindFirstChildOfClass("Humanoid")

        local function onJumpRequest()
            if humanoid then
                humanoid:ChangeState("Jumping")
            end
        end

        infiniteJumpConnection = game:GetService("UserInputService").JumpRequest:Connect(onJumpRequest)
        InfiniteJumpButton.Text = "Infinite Jump Enabled"
        InfiniteJumpButton.BackgroundColor3 = Color3.fromRGB(85, 255, 127)
    else
        infiniteJumpEnabled = false
        InfiniteJumpButton.Text = "Enable Infinite Jump"
        InfiniteJumpButton.BackgroundColor3 = Color3.fromRGB(100, 177, 0)
        if infiniteJumpConnection then
            infiniteJumpConnection:Disconnect()
        end
    end
end)

CopyDiscordButton.MouseButton1Click:Connect(function()
    setclipboard("https://discord.gg/t6UgyY65")
    CopyDiscordButton.Text = "Copied!"
    wait(2)
    CopyDiscordButton.Text = "Copy Discord Link"
end)
